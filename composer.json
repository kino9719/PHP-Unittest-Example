{
    "name": "demo/php-unittest-example",
    "autoload": {
        "psr-4": {
            "Demo\\PhpUnittestExample\\": "src/",
            "Demo\\PhpUnittestExample\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
            "pre-commit install --hook-type pre-commit --hook-type pre-push"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6"
    },
    "require": {
        "guzzlehttp/guzzle": "^7.8"
    }
}
